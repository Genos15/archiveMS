type Article {
    id: ID!
    name: String!
    model: String
    series: String
    brand: String
    description: String!
    price: Long!
    isSold: Boolean!
    citiesAvailability: [String!]!
    images: [ID!]!
    createdAt: DateTime!
    editedAt: DateTime!
}

type ArticleEdge {
    cursor: String
    node: Article
}

type ArticleConnection {
    edges: [ArticleEdge]
    pageInfo: PageInfo
}

